platform :ios, '10.0'

project 'FrameOk.xcodeproj'

def pods

    # Network

    pod 'Alamofire', '~> 4.9'
    pod 'AlamofireNetworkActivityIndicator', '~> 2.4'
    pod 'AlamofireNetworkActivityLogger'
    pod 'Kingfisher'

    # Utils

    pod 'PhoneNumberKit'
    pod 'XCGLogger', '~> 6.1.0'
    pod 'GCDWebServer', '~> 3.0'

    # UI

    pod "SkeletonView"
    pod 'SwiftEntryKit'
    pod 'InputMask', '~> 4.3.0'

end
    

target 'FrameOk' do

    use_frameworks!

    pods
end